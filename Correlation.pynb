    "%matplotlib inline\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import scipy.stats\n",
    "\n",
    "x = np.random.rand(100)\n",
    "y = -10*x + np.random.randn(100)\n",
    "corr = scipy.stats.pearsonr(x,y)[0]\n",
    "\n",
